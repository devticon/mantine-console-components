{
  "name": "@devticon/mantine-console-components",
  "version": "4.0.3",
  "main": "./dist/cjs/main.js",
  "module": "./dist/esm/main.js",
  "types": "./dist/esm/main.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --project tsconfig.json --module ESNext --outDir ./dist/esm",
    "build:cjs": "tsc --project tsconfig.json --module CommonJS --outDir ./dist/cjs",
    "format": "prettier . --write --ignore-path .gitignore",
    "lint": "eslint . --fix --ignore-path .gitignore"
  },
  "peerDependencies": {
    "@mantine/core": "^7.0.0",
    "@mantine/dates": "^7.0.0",
    "@mantine/modals": "^7.0.0",
    "@mantine/notifications": "^7.0.0",
    "@remix-run/node": "^1.0.0 || ^2.0.0",
    "@remix-run/react": "^1.0.0 || ^2.0.0",
    "dayjs": "^1.11.10",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-fs-backend": "^2.3.1",
    "i18next-http-backend": "^2.4.2",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-icons": "^4.0.0 || ^5.0.0",
    "remix-i18next": "^5.5.0",
    "zod": "^3.22.4",
    "zod-form-data": "^2.0.2",
    "zod-i18n-map": "^2.21.0"
  },
  "dependencies": {
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@remix-run/eslint-config": "^2.4.1",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.7",
    "@types/react": "^18.2.47",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*": "prettier --write -u"
  }
}
